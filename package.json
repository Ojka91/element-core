  "devDependencies": {
    "@types/body-parser": "1.19.0",
    "@types/cors": "2.8.7",
    "@types/express": "4.17.7",
    "@types/node": "14.0.27",
    "concurrently": "5.3.0",
    "nodemon": "2.0.4",
    "ts-node": "8.10.2",
    "typescript": "4.6.2"
